{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://spica.internal/passport/refreshtoken",
  "type": "object",
  "required": ["token"],
  "properties": {
    "_id": {
      "type": "string",
      "format": "objectid",
      "description": "Unique objectid of the token"
    },
    "identity": {
      "type": "string",
      "description": "identity"
    },
    "token": {
      "type":"string",
      "description": "token"
    },
    "expires_in": {
      "type":"string",
      "description": "date"
    }
  }
}
